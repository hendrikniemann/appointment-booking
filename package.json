{
  "name": "with-chakra-ui-typescript",
  "version": "1.0.0",
  "scripts": {
    "build": "rm -rf build && cp -r public build && esbuild src/index.tsx --outdir=build --define:process.env.NODE_ENV=\"'production'\" --bundle",
    "start": "esbuild src/index.tsx --servedir=public --outdir=public --bundle --define:process.env.NODE_ENV=\"'development'\""
  },
  "dependencies": {
    "@chakra-ui/icons": "^1.0.5",
    "@chakra-ui/react": "^1.3.3",
    "@chakra-ui/theme-tools": "1.0.4",
    "@emotion/react": "11.1.5",
    "@emotion/styled": "11.1.5",
    "date-fns": "^2.19.0",
    "framer-motion": "^3.5.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-icons": "^4.2.0",
    "react-query": "^3.13.0"
  },
  "devDependencies": {
    "@types/node": "^14.6.0",
    "@types/react": "^16.9.46",
    "@types/react-dom": "^16.9.8",
    "esbuild": "^0.9.6",
    "typescript": "4.0.5"
  },
